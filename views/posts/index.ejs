<h1>Posts Index</h1>
<% posts.forEach(function(post){ %>
<div >
    <h2><%=post.title %></h2>
    <div>Price: <%=post.title%></div>
    <div>Description: <%=post.description %></div>
    <div>Location: <%=post.location %></div>
    <div><a href="/posts/<%= post.id %>">View more</a></div>
</div >
<% }); %>
